    "type": "prestashop-module",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "prestashop/php-dev-tools": "^4.3 || ^3.16",
        "phpunit/phpunit": "^7 || ^8 || ^9",
        "prestashop/autoindex": "^2.1 || ^1",
        "friendsofphp/php-cs-fixer": "^3.52 || ^2",
        "lchrusciel/api-test-case": "^5.3 || ^4.1"
    },
    "autoload": {
